                "number": 81,
                "title": "Can't disable HTML escape when using lazy translation",
                "user": {
                    "login": "haroal",
                    "id": 24915462,
                    "node_id": "MDQ6VXNlcjI0OTE1NDYy",
                    "avatar_url": "https://avatars.githubusercontent.com/u/24915462?v=4",
